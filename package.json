{
  "dependencies": {
    "@nextui-org/react": "^2.1.13",
    "bunrest": "^1.3.6",
    "framer-motion": "^10.16.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "name": "pasma",
  "module": "server/main.ts",
  "type": "module",
  "scripts": {
    "client": "bun build ./client/Main.jsx --outdir ./public --minify",
    "client:dev": "bun build ./client/Main.jsx --outdir ./public --watch --sourcemap=external",
    "server": "bun run server/main.ts",
    "server:dev": "bun run --watch server/main.ts",
    "css": "bunx tailwindcss -i ./public/styles.css -o ./public/main.css",
    "css:dev": "bunx tailwindcss -i ./public/styles.css -o ./public/main.css --watch",
    "clean": "rm ./public/main.*"
  },
  "devDependencies": {
    "bun-types": "latest",
    "tailwindcss": "^3.3.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
